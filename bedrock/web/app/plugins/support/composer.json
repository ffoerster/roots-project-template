{
  "name": "roots/tiny-pixel-support",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "Support",
  "homepage": "https://roots.io",
  "keywords": [
    "wordpress",
    "plugin",
    "roots"
  ],
  "autoload": {
    "psr-4": {
      "TinyPixel\\Support\\": ["app/", "boot/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TinyPixel\\Support\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1",
    "composer/installers": "^1.0",
    "roots/acorn": "dev-master",
    "roots/support": "dev-master",
    "illuminate/support": "^5.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "squizlabs/php_codesniffer": "^3.3",
    "symfony/var-dumper": "^4.2",
    "paulgibbs/behat-wordpress-extension": "^3.3",
    "behat/mink-goutte-driver": "^1.2",
    "dmore/behat-chrome-extension": "^1.3"
  },
  "scripts": {
    "lint": "phpcs"
  }
}
